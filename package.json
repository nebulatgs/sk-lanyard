{
	"name": "sk-lanyard",
	"version": "0.2.4",
	"description": "SvelteKit integration with Lanyard, an API to fetch your Discord presence",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": "https://github.com/nebulatgs/sk-lanyard",
	"author": "Nebula <infinitewormhole@gmail.com>",
	"bugs": {
		"url": "https://github.com/nebulatgs/sk-lanyard/issues"
	},
	"files": [
		"dist",
		"package.json"
	],
	"keywords": [
		"typescript",
		"svelte",
		"sveltekit",
		"lanyard",
		"discord",
		"presence"
	],
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "ncc build src/index.ts -m -e svelte/store",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"up": "tsup src/index.ts --dts --format esm,cjs --external svelte/store",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"@vercel/ncc": "^0.38.0",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2"
	},
	"peerDependencies": {
		"svelte": "^4.2.0",
		"vite": "^4.4.9"
	},
	"dependencies": {
		"vite": "^4.4.9"
	}
}
